{
  "name": "package",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",

    "compile:sass": "node-sass public/sass/main.scss public/stylesheets/main.css -w",

    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyang77/e-commerce-website.git"
  },
  "author": "haoli and adi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hyang77/e-commerce-website/issues"
  },
  "homepage": "https://github.com/hyang77/e-commerce-website#readme",
  "dependencies": {
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}
